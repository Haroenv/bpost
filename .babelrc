{
  "env": {
    "cjs": {
      "presets": [
        [
          "es2015",
          {
            "loose": true
          }
        ]
      ],
      "plugins": [
        "add-module-exports",
        "transform-flow-strip-types"
      ]
    },
    "es": {
      "presets": [
        [
          "es2015",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-flow-strip-types"
      ]
    },
    "test": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "transform-async-to-generator"
      ]
    }
  }
}
