{
  "env": {
    "cjs": {
      "presets": [
        ["es2015", {"loose": true}]
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [
        ["es2015", {"loose": true, "modules": false}]
      ]
    },
    "test": {
      "presets": ["es2015"],
      "plugins": ["transform-async-to-generator"]
    }
  }
}
